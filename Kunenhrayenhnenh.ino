void drawUpArrow(uint8_t cX, uint8_t cY) {
  arduboy.drawLine(cX, cY-2, cX, cY-6, BLACK);
  arduboy.drawLine(cX, cY-6, cX-2, cY-4, BLACK);
  arduboy.drawLine(cX, cY-6, cX+2, cY-4, BLACK);
}

void drawDownArrow(uint8_t cX, uint8_t cY) {
  arduboy.drawLine(cX, cY+2, cX, cY+6, BLACK);
  arduboy.drawLine(cX, cY+6, cX-2, cY+4, BLACK);
  arduboy.drawLine(cX, cY+6, cX+2, cY+4, BLACK);
}

void drawLeftArrow(uint8_t cX, uint8_t cY) {
  arduboy.drawLine(cX-2, cY, cX-6, cY, BLACK);
  arduboy.drawLine(cX-6, cY, cX-4, cY-2, BLACK);
  arduboy.drawLine(cX-6, cY, cX-4, cY+2, BLACK);
}

void drawRightArrow(uint8_t cX, uint8_t cY) {
  arduboy.drawLine(cX+2, cY, cX+6, cY, BLACK);
  arduboy.drawLine(cX+6, cY, cX+4, cY-2, BLACK);
  arduboy.drawLine(cX+6, cY, cX+4, cY+2, BLACK);
}

void drawTopText(const char* text) {
  if (text == 0)
  {
    return;
  }

  arduboy.setTextSize(1);
  arduboy.setCursor(0,0);
  arduboy.setTextColor(WHITE);
  arduboy.setTextBackground(BLACK);
  arduboy.print(text);
}

void drawText(const char* text) {
  if (text == 0)
  {
    return;
  }

  arduboy.setTextSize(1);
  byte l = strlen(text);
  int xoffset = 128/2-l*6/2;

  arduboy.setCursor(xoffset,HEIGHT-8);
  arduboy.setTextColor(WHITE);
  arduboy.print(text);
}

void drawCredits() {
  char* text[5];
  text[0] = "   a game by";
  text[1] = "Tyler Wright";
  text[2] = "";
  text[3] = "   ported by";
  text[4] = "Dylan Wright";
  arduboy.setTextSize(1);
  arduboy.setTextColor(WHITE);

  for (size_t i = 0; i < 5; ++i) {
    arduboy.setCursor(1,10*i);
    arduboy.print(text[i]);
  }
  
}

void gotoRoom(uint8_t offset) {
  text = 0;
  uint8_t nextRoom = directions[pageIndex*4+offset];
  if (nextRoom == 0) {
    text = wrongWay;
    return;
  }
  pageIndex = nextRoom;
  showCompass = true;
}

void setup() {
  arduboy.begin();
}

void loop() {
  arduboy.pollButtons();
  arduboy.clear();

  if (pageIndex > 0 && pageIndex < 18) {
    if (arduboy.justPressed(RIGHT_BUTTON)) {
      gotoRoom(0);
    }
    else if (arduboy.justPressed(UP_BUTTON)) {
      gotoRoom(1);
    }
    else if (arduboy.justPressed(DOWN_BUTTON)) {
      gotoRoom(2);
    }
    else if (arduboy.justPressed(LEFT_BUTTON)) {
      gotoRoom(3);
    }
  }
  else {
    if (arduboy.justPressed(A_BUTTON) || arduboy.justPressed(B_BUTTON)) {
      if (pageIndex == 0) {
        showCompass = true;
      }
      pageIndex = directions[pageIndex*4];
    }
  }

  arduboy.drawCompressed(0, 0, pgm_read_word_near(screens+pageIndex));
  arduboy.fillRect(0, HEIGHT-9, WIDTH, HEIGHT, BLACK);

  // Title
  if (pageIndex == 0) {
    drawTopText("Kunenhrayenhnenh");
    drawText("begin");
  }
  // Ending
  else if (pageIndex >= 18) {
    showCompass = false;
    switch (pageIndex) {
      case 18:
        drawText("!!!");
        break;
      case 19:
      case 20:
      case 21:
      case 22:
        drawText("...");
        break;
      case 23:
        drawCredits();
        drawText("restart");
        break;
      case 24:
        drawTopText("Kunenhrayenhnenh");
        drawText("credits");
        break;
    }
  } else {
    drawText(text);
  }

  if (showCompass) {
    uint8_t cX = WIDTH - 10;
    uint8_t cY = 9;
    arduboy.fillRect(WIDTH-19, 0, 19, 19, BLACK);
    arduboy.drawRect(WIDTH-19, 0, 19, 19, WHITE);
    arduboy.fillCircle(cX, cY, 7);

    if (directions[pageIndex*4+0] != 0) drawRightArrow(cX, cY);
    if (directions[pageIndex*4+1] != 0) drawUpArrow(cX, cY);
    if (directions[pageIndex*4+2] != 0) drawDownArrow(cX, cY);
    if (directions[pageIndex*4+3] != 0) drawLeftArrow(cX, cY);
  }

  arduboy.display();
}
